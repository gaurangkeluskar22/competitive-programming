/*
// Definition for a Node.
class Node {
public:
    int val;
    vector<Node*> children;

    Node() {}

    Node(int _val) {
        val = _val;
    }

    Node(int _val, vector<Node*> _children) {
        val = _val;
        children = _children;
    }
};
*/
#include <bits/stdc++.h>
using namespace std;

class Solution {
public:
    vector<int> preorder(Node* root) {
        vector<int> res;
        stack<Node*> s;
        if(root==nullptr){
            return res;
        }
        s.push(root);
        while(!s.empty()){
            Node* front=s.top();
            s.pop();
            res.push_back(front->val);
            for(int i=front->children.size()-1;i>=0;i--){
                if(front->children[i]!=nullptr){
                    s.push(front->children[i]);
                }
            }
        }
 
        return res;
    }
};
