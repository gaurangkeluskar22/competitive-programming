class Solution:
    def maxArea(self, height: List[int]) -> int:
        ans=[]
        i=0
        j=len(height)-1
        while(i<j):
            ans.append(min(height[i],height[j])*(j-i))
            if(height[i]>height[j]):
                j-=1
            else:
                i+=1
        return max(ans)
        
        
