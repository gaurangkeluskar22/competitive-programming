# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, val=0, left=None, right=None):
#         self.val = val
#         self.left = left
#         self.right = right
class Solution:
    def insertIntoBST(self, root: TreeNode, val: int) -> TreeNode:
        temp=root
        new_node = TreeNode(val)
        if not root:
             return new_node
        else:
            while(True):
                if(root.val<val):
                    if not root.right:
                        root.right=new_node
                        break
                    else:
                        root=root.right
                else:
                    if not root.left:
                        root.left=new_node
                        break
                    else:
                        root=root.left

            return temp
