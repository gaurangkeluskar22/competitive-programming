# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def isPalindrome(self, head: ListNode) -> bool:
        string=""
        while(head!=None):
            string+=str(head.val)
            head=head.next
        
        if(string==string[::-1]):
            return True
        else:
            return False
        
